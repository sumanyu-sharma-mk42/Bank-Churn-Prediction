{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/app.py"
      }
    ],
    "functions": {
      "api/index.py": {
        "runtime": "python@3.10"
      }
    }
  }
  